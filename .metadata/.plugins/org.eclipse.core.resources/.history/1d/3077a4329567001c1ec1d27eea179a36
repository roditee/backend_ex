package sec05;

import java.util.Date;

public class BookVO {
	private String no;
	private String name;
	private String author;
	private int price;
	private Date date;
	private int stock;
	private String pubNo;
	
	// 디폴트 생성자
		public BookVO() { }
		
	// 현재는 매개변수 있는 생성자 필요 없음
		
	public BookVO(String no, String name, String author, int price, Date date, int stock, String pubNo) {
		this.no = no;
		this.name = name;
		this.author = author;
		this.price = price;
		this.date = date;
		this.stock = stock;
		this.pubNo = pubNo;
	}
}
